{
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        },
        {
            "src": "api/cron/keep-alive.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/index.js"
        },
        {
            "src": "/test",
            "dest": "/index.js"
        },
        {
            "src": "/health",
            "dest": "/index.js"
        }
    ],
    "crons": [
        {
            "path": "/api/cron/keep-alive",
            "schedule": "0 */10 * * *"
        }
    ]
}